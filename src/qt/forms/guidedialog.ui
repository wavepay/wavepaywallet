&lt;p style=&quot; margin-top:12px; margin-bottom:12px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;&quot;&gt;sudo add-apt-repository ppa:bitcoin/bitcoin &lt;/p&gt;
